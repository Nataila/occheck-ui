ssage.error('请输入邮箱')\n        return false\n      }\n      const res = await api.sendCode({email})\n      if (res) {\n        message.success('已发送！');\n        count -= 1\n        setCodeVal(`${count}s后重新发送`)\n        setCounting(true)\n        countDown()\n      }\n    }\n  }\n\n  return ( \n    <div className=\"signup-root\">\n      <div className=\"signup-content oc-shadow\">\n        <div className=\"title text-center\">忘记密码</div>\n        <div className=\"logo-color-wrapper\">\n          <img src={ logoColor } alt=\"\" />\n        </div>\n        <Form\n          onFinish={onFinish}\n          style={{width: 'min-content', margin: '0 auto', position: 'relative'}}>\n          <Form.Item\n            name=\"email\"\n            rules={[{ required: true, message: '请输入邮箱' }]}\n          >\n          <Input\n            type='email'\n            className=\"signup-input\"\n            placeholder=\"请输入邮箱\"\n            name='email'\n            onChange={e => {setEmail(e.target.value)}}\n            />\n          </Form.Item>\n\n          <Form.Item\n            name=\"code\"\n            rules={[{ required: true, message: '请输入验证码' }]}\n          >\n          <Input\n            type='text'\n            className=\"signup-input\"\n            placeholder=\"验证码\"\n            name='code'\n            />\n          </Form.Item>\n\n          <Form.Item\n            name=\"new_password1\"\n            rules={[{ required: true, message: '重置您的密码' }]}\n          >\n            <Input\n              type=\"password\"\n              className=\"signup-input\"\n              placeholder=\"重置您的密码\"\n              name='new_password1' />\n          </Form.Item>\n\n          <Form.Item\n            name=\"new_password2\"\n            rules={[{ required: true, message: '请再次输入密码' }]}\n          >\n            <Input\n              type=\"password\"\n              className=\"signup-input\"\n              placeholder=\"请再次输入密码\"\n              name='new_password2' />\n          </Form.Item>\n          <span className='send-code' onClick={countDownFunc}>{ codeVal }</span>\n          <button className=\"oc-btn-primary submit-btn\" htmlType=\"submit\">提交</button>\n        </Form>\n      </div>\n    </div>\n  )\n}\n","/opt/outsource/occheck/ui/src/pages/Admin/SysConf/index.jsx",["208","209","210"],{"ruleId":"211","replacedBy":"212"},{"ruleId":"213","replacedBy":"214"},{"ruleId":"215","severity":1,"message":"216","line":27,"column":9,"nodeType":"217","messageId":"218","endLine":27,"endColumn":15},{"ruleId":"215","severity":1,"message":"219","line":27,"column":17,"nodeType":"217","messageId":"218","endLine":27,"endColumn":23},{"ruleId":"215","severity":1,"message":"220","line":27,"column":25,"nodeType":"217","messageId":"218","endLine":27,"endColumn":32},{"ruleId":"221","severity":1,"message":"222","line":17,"column":27,"nodeType":"223","messageId":"224","endLine":17,"endColumn":29},{"ruleId":"225","severity":1,"message":"226","line":60,"column":13,"nodeType":"227","endLine":60,"endColumn":46},{"ruleId":"215","severity":1,"message":"228","line":8,"column":8,"nodeType":"217","messageId":"218","endLine":8,"endColumn":18},{"ruleId":"215","severity":1,"message":"229","line":18,"column":8,"nodeType":"217","messageId":"218","endLine":18,"endColumn":18},{"ruleId":"215","severity":1,"message":"230","line":4,"column":40,"nodeType":"217","messageId":"218","endLine":4,"endColumn":46},{"ruleId":"215","severity":1,"message":"231","line":4,"column":56,"nodeType":"217","messageId":"218","endLine":4,"endColumn":62},{"ruleId":"215","severity":1,"message":"232","line":5,"column":10,"nodeType":"217","messageId":"218","endLine":5,"endColumn":27},{"ruleId":"215","severity":1,"message":"233","line":5,"column":29,"nodeType":"217","messageId":"218","endLine":5,"endColumn":41},{"ruleId":"215","severity":1,"message":"234","line":12,"column":9,"nodeType":"217","messageId":"218","endLine":12,"endColumn":15},{"ruleId":"215","severity":1,"message":"235","line":15,"column":10,"n